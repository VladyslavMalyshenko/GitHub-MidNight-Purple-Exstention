{
    "manifest_version": 3,
    "name": "GitHub MidNight Purple Theme",
    "version": "0.1.0",
    "content_scripts": [
        {
            "matches": ["*://github.com/*"],
            "js": ["script.js"],
            "css": ["style.css"]
        }
    ],
    "permissions": [
        "storage"
    ],
    "icons": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    }
}
