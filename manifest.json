{
    "manifest_version": 2,
    "name": "GitHub MidNight Purple Theme",
    "version": "0.1.0",
    "content_scripts": [
        {
            "matches": ["*://github.com/*"],
            "js": ["script.js"],
            "css": ["style.css"]
        }
    ],
    "permissions": [
        "storage"
    ]
}